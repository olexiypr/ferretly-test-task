{
    "Serilog": {
        "Using": [ "Serilog.Sinks.Console", "Serilog.Sinks.File" ],
        "MinimumLevel": "Verbose",
        "WriteTo": [
            {
                "Name": "File",
                "Args": {
                    "path": "logs\\Serilog..log",
                    "rollingInterval": "Month"
                }
            }
        ],
        "Properties": {
            "Application": "SerilogVSNlog"
        }
    }
}